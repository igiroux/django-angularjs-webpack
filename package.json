{
  "name": "django-angularjs-webpack",
  "version": "1.0.0",
  "description": "A boiletplate for AngularJS built with webpack and Django Framework",
  "main": "resources/assets/src/app/index.js",
  "dependencies": {
    "angular": "^1.6.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.0",
    "less-loader": "^4.0.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "scss-loader": "0.0.1",
    "style-loader": "^0.17.0",
    "stylus-loader": "^3.0.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server.cmd --content-base resources/assets/js/src/app",
    "build": "set NODE_ENV=production && \"node_modules/.bin/webpack.cmd\" && cp resources/assets/js/src/app/index.html webapp/templates/webapp/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matoxxx/django-angularjs-webpack.git"
  },
  "keywords": [
    "django",
    "webpack",
    "angular",
    "boilerplate",
    "template"
  ],
  "author": "Matej Hocevar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matoxxx/django-angularjs-webpack/issues"
  },
  "homepage": "https://github.com/matoxxx/django-angularjs-webpack#readme"
}
